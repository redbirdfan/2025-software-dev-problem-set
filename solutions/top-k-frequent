//https://leetcode.com/problems/top-k-frequent-elements/

/**
 * @param {number[]} nums
 * @param {number} k
 * @return {number[]}
 */
var topKFrequent = function(nums, k) {

    let tally = {};
    
    for (let i = 0 ; i < nums.length ; i++){
        let num = nums[i];

        if(tally[num]){
            tally[num]++;
        } else {
            tally[num] = 1;
        } 
    }
        let tallyToArray = Object.entries(tally);

        tallyToArray.sort((a, b) => b[1] - a[1]); //This b-a allows sorting descending and a - b would be ascending

        let topOccuring = tallyToArray.slice(0, k);

        let mostFrequent = topOccuring.map(entry => Number(entry[0]));

        return mostFrequent;
